    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0"
  },
  "now": {
    "alias": "case-study.anthony.codes",
    "engines": {
      "node": "^10.0.0"
    },
    "env": {
      "NODE_ENV": "production"
    },
    "name": "case-study",
    "public": true,
    "type": "npm"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
