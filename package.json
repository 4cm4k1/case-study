{
  "name": "case-study",
  "version": "0.0.1",
  "private": true,
  "description": "Case study for job application",
  "bugs": "https://github.com/4cm4k1/case-study/issues",
  "repository": "https://github.com/4cm4k1/case-study",
  "license": "MIT",
  "author": "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>",
  "contributors": [
    "Anthony Maki (https://anthony.codes) <4cm4k1@gmail.com>"
  ],
  "main": "server.js",
  "scripts": {
    "build": "next build",
    "dev": "NODE_ENV=dev node server.js",
    "precommit": "pretty-quick --staged",
    "start": "NODE_ENV=production node server.js",
    "prettier": "prettier '{*,{lib,test,web}/**/*}.{css,js,json,md,scss}' --write",
    "prettier:watch": "onchange '{*,{lib,test,web}/**/*}.{css,js,json,md,scss}' -- prettier --write {{changed}}",
    "watch": "npm-run-all prettier-watch dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.15.0-0",
    "@zeit/next-bundle-analyzer": "^0.1.1",
    "@zeit/next-mdx": "^1.1.0",
    "@zeit/next-source-maps": "^0.0.2",
    "next": "^6.1.1-canary.2",
    "next-compose-plugins": "^2.1.1",
    "next-offline": "^2.8.0",
    "next-optimized-images": "^1.4.1",
    "polka": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shrink-ray-current": "^2.1.2"
  },
  "devDependencies": {
    "@types/next": "^6.1.0",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "debug": "^3.1.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0"
  },
  "now": {
    "alias": "case-study.anthony.codes",
    "engines": {
      "node": "^10.0.0"
    },
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      ".next/",
      "server.js"
    ],
    "name": "anthony-codes",
    "public": true,
    "type": "npm"
  },
  "renovate-config": {
    "assignees": [
      "4cm4k1"
    ],
    "extends": [
      "config:base",
      ":pinAllExceptPeerDependencies"
    ],
    "labels": [
      "renovate"
    ]
  }
}
